from Compiler.types import sint, sfix, Array
from Compiler.library import print_ln

m = 1000

a = Array(m, sint)
b = Array(m, sint)
c = Array(m, sint)
d = Array(m, sint)

a.input_from(0)
b.input_from(1)

c[:] = a[:]*b[:]
d[:] = a[:] + b[:] - c[:]

J = sfix(sum(c)) / sfix(sum(d))

print_ln('%s', J.reveal())
